
  <% @djs.each do |dj| %>
    <li>
      <%= link_to dj.name, dj_path(dj) %>
      <%= dj.genre.category %>
      <%= dj.rate %>
      <%= dj.description %>
      <% if dj.photo.attached? %>
        <%= image_tag dj.photo, class: 'dj-photo' %>
      <% else %>
        <p>No photo available.</p>
      <% end %>
    </li>
  <% end %>

